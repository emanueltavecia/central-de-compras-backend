{
  "name": "central-de-compras-backend",
  "version": "1.0.0",
  "description": "Back-end de MVP de uma Central de Compras desenvolvido para fins acadêmicos.",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && tsc-alias && resolve-tspaths",
    "start": "node dist/server.js",
    "lint": "prettier --write ./src && eslint ./src --fix",
    "type-check": "tsc --noEmit",
    "db:test": "tsx db-test.ts",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f postgres",
    "docker:remove": "docker compose down -v"
  },
  "keywords": [],
  "author": "Emanuel Cardoso Tavecia, Guilherme Conti Machado, Gabriel Alves Teixeira, Caio Vinícius Guimarães de Oliveira Dagostim",
  "license": "Veja licença em LICENSE.md",
  "packageManager": "pnpm@10.12.4",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "cpf-cnpj-validator": "^1.0.3",
    "decimal.js": "^10.6.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "humps": "^2.0.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "routing-controllers": "^0.11.3",
    "swagger-ui-express": "^5.0.1",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@emanueltavecia/eslint-config": "1.0.5",
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/humps": "^2.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.5.1",
    "@types/pg": "^8.15.5",
    "@types/multer": "^1.4.11",
    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "8.57.1",
    "resolve-tspaths": "^0.8.23",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
